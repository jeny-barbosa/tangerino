{
  "manifest_version": 2,
  "name": "Pontos Tangerino",
  "version": "1.8.8.2",
  "description": "Importar para .xlsx informações dos pontos registrados.",
  "icons": {
    "128": "icone.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*",
    "*://*/*",
    "background"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "*://*/*",
        "https://app.tangerino.com.br/*"],
      "js": [
        "jquery-1.9.1.min.js",
        "xlsx.core.min.js",
        "Blob.min.js",
        "FileSaver.min.js",
        "tableexport.min.js",
        "background.js",
        "popup.js"
      ]
    }
  ]
}
